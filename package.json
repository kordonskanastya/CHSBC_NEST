{
   "name": "csbc",
   "version": "1.0.0",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "prebuild": "rimraf dist",
      "build": "nest build",
      "server": "node dist/main",
      "dev": "nodemon --config nodemon.json",
      "prod": "nodemon --config nodemon.prod.json",
      "start:debug": "env-cmd -f .env.local nest start --debug --watch",
      "start:local": "env-cmd -f .env.local nest start --watch",
      "start:dev": "env-cmd -f .env.dev nest start --watch",
      "start:stage": "env-cmd -f .env.stage nest start --watch",
      "server:local": "env-cmd -f .env.local node dist/main",
      "lint": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "test:user": "jest -t UserService --detectOpenHandles",
      "dev:db": "./src/scripts/sh/start-db.sh",
      "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/ts/orm-config.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "drop:schema:local": "env-cmd -f .env.local npm run typeorm -- schema:drop",
      "drop:schema:dev": "env-cmd -f .env.dev npm run typeorm -- schema:drop",
      "drop:schema:stage": "env-cmd -f .env.stage npm run typeorm -- schema:drop",
      "migration": "npm run typeorm -- migration:run",
      "migration:local": "env-cmd -f .env.local npm run typeorm -- migration:run",
      "migration:dev": "env-cmd -f .env.dev npm run typeorm -- migration:run",
      "migration:stage": "env-cmd -f .env.stage npm run typeorm -- migration:run",
      "migration:revert:local": "env-cmd -f .env.local npm run typeorm -- migration:revert",
      "migration:revert:dev": "env-cmd -f .env.dev npm run typeorm -- migration:revert",
      "migration:revert:stage": "env-cmd -f .env.stage npm run typeorm -- migration:revert",
      "add:root": "env-cmd -f .env.local npx ts-node src/scripts/ts/add_root.ts",
      "start": "ts-node src/index.ts"
   },
   "dependencies": {
      "@nestjs-modules/mailer": "^1.6.0",
      "@nestjs/common": "^8.0.0",
      "@nestjs/core": "^8.0.0",
      "@nestjs/jwt": "^8.0.0",
      "@nestjs/passport": "^8.0.1",
      "@nestjs/platform-express": "^8.0.0",
      "@nestjs/schedule": "^1.0.2",
      "@nestjs/swagger": "^5.1.0",
      "@nestjs/typeorm": "^8.0.2",
      "@types/faker": "^5.5.8",
      "faker": "^5.5.3",
      "bcrypt": "^5.0.1",
      "class-transformer": "0.4.0",
      "class-validator": "^0.13.1",
      "country-code-switch": "^1.0.1",
      "country-state-city": "^3.0.1",
      "env-cmd": "^10.1.0",
      "moment": "^2.29.1",
      "nest-winston": "^1.6.1",
      "nestjs-pino": "^2.5.0",
      "nestjs-swagger-api-implicit-queries-decorator": "^1.0.0",
      "nestjs-typeorm-paginate": "^3.1.3",
      "node-geocoder": "^3.27.0",
      "nodemailer": "^6.7.0",
      "nodemon": "^2.0.13",
      "passport": "^0.4.1",
      "passport-jwt": "^4.0.0",
      "passport-local": "^1.0.0",
      "pg": "^8.7.1",
      "pino-http": "^6.6.0",
      "pino-pretty": "^7.5.4",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.2.0",
      "sharp": "^0.30.2",
      "swagger-ui-express": "^4.1.6",
      "typeorm": "0.2.44",
      "winston": "^3.3.3",
      "winston-daily-rotate-file": "^4.5.5",
      "mysql": "^2.14.1"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.0.0",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@types/bcrypt": "^5.0.0",
      "@types/cron": "^1.7.3",
      "@types/express": "^4.17.13",
      "@types/geolib": "^2.0.23",
      "@types/jest": "^27.0.1",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/node-geocoder": "^3.24.3",
      "@types/nodemailer": "^6.4.4",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^4.28.2",
      "@typescript-eslint/parser": "^4.28.2",
      "dotenv": "^11.0.0",
      "eslint": "7.30.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.0",
      "jest": "^27.0.6",
      "prettier": "^2.3.2",
      "supertest": "^6.1.3",
      "ts-jest": "^27.0.3",
      "ts-loader": "^9.2.3",
      "ts-node": "10.4.0",
      "tsconfig-paths": "^3.10.1",
      "typescript": "4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
